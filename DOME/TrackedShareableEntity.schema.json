{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "$id": "#TrackedShareableEntity",
    "title": "TrackedShareableEntity",
    "description": "DOME extensions for TMForum entities that needs to be shared across Access Nodes",
    "properties": {
        "lastUpdate": {
            "type": "string",
            "format": "date-time",
            "description": "Date and time of the last update"
        },
        "relatedParty": {
            "type": "array",
            "description": "List of related parties",
            "items": {
                "$ref": "../schemas/EngagedParty/RelatedParty.schema.json#RelatedParty"
            }
        }
    }
}